
pipeline {
  agent any
  stages {
  stage('Stage 1') {
      steps {
        script {
          echo 'Stage 1'
        }
      }
    }
  stage('Deploy') {
      steps {
        script {
                sh 'echo DEPLOYING...'
                sh 'vagrant up --provision'
                sh 'echo DEPLOY STAGE OK'     
        }
      }
    }
  }
}
pipeline {
    agent any
    stages {
        stage('Build'){
            steps {
                sh 'echo BUILDING...'
                sh 'echo BUILD ID - ${BUILD_ID}'
                sh 'echo BUILD STAGE OK'
            }
        }
        stage('Test'){
            steps {
                sh 'echo TESTING...'
		            sh 'python3 test.py'
            }
        }
        stage('Deploy'){
            steps {
                sh 'echo DEPLOYING...'
                sh 'cd /var/lib/jenkins/workspace/default_develop/VAW Centos/'
                sh 'vagrant up --provision'
                sh 'echo DEPLOY STAGE OK'
            }
        }
    }
}
