
pipeline {
    agent any
    stages {
        stage('Build'){
            steps {
                sh 'echo BUILDING...'
                sh 'echo BUILD ID - ${BUILD_ID}'
                sh 'echo BUILD STAGE OK'
            }
        }
        stage('Deploy'){
            steps {
                sh 'echo DEPLOYING...'
                sh 'sudo -S Wolkforever1 vagrant up --provision'
                sh 'echo DEPLOY STAGE OK'
            }
        }
    }
}
