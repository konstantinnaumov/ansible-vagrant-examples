pipeline {
    agent any
    stages {
        stage('Build'){
            steps {
                sh 'echo BUILDING...'
                sh 'echo BUILD ID - ${BUILD_ID}'
                sh 'echo BUILD STAGE OK'
            }
        }
        stage('Deploy'){
            steps {
                sh 'echo DEPLOYING...'
                sh 'cd /var/lib/jenkins/workspace/default_develop/VAW_Centos/ | vagrant up | vagrant provision'
                sh 'echo DEPLOY STAGE OK'
            }
        }
    }
}
